{
    "name": "stef/dagvandeweek",
    "license": "Not decided yet",
    "type": "project",
    "description": "Website and CMS for the youth Scouting Brigitta / Valentijn - The Hague - Netherlands",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.10.2",
                "dist": {
                    "url": "https://code.jquery.com/jquery-1.10.2.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/stefanius/simple-cms-bundle"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "~3.0",
        "doctrine/orm": "~2.5",
        "doctrine/dbal": "~2.5",
        "doctrine/doctrine-bundle": "~1.2",
        "phpunit/phpunit": "~4.1",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "braincrafted/bootstrap-bundle": "~2.0",
        "jms/serializer-bundle": "dev-master",
        "nelmio/alice": "~2.0",
        "egeloen/ckeditor-bundle": "~2.0",
        "whiteoctober/breadcrumbs-bundle": "~1.0",
        "stef/crud-contract": "dev-master",
        "stef/key-value-manipulation": "dev-master",
        "stef/slug-manipulation": "dev-master",
        "stefanius/simple-cms-bundle": "dev-master",
        "stef/redirect-trailing-slash-bundle": "dev-master",
        "stef/special-dates": "dev-master",
        "laravel/homestead": "^3.0",
        "symfony/debug": "^3.0"
    },
    "scripts": {
      "post-root-package-install": [
        "SymfonyStandard\\Composer::hookRootPackageInstall"
      ],
      "post-install-cmd": [
        "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
      ],
      "post-update-cmd": [
        "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
      ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
