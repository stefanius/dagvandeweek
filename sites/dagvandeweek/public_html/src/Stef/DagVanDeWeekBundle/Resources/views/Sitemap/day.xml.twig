{%
    set months = {
        1: 'januari',
        2: 'februari',
        3: 'maart',
        4: 'april',
        5: 'mei',
        6: 'juni',
        7: 'juli',
        8: 'augustus',
        9: 'september',
        10: 'oktober',
        11: 'november',
        12: 'december',
    }
%}
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for record in records %}
        <url>
            <loc>http://dagvandeweek.nl{{ path('stef_dag_van_de_week_day_by_day', {
                    'day': record.day,
                    'dutchMonthName': months[record.month],
                }) }}</loc>
            <changefreq>daily</changefreq>
            <priority>1</priority>
        </url>
    {% endfor %}
</urlset>